        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "better-npm-run start",
        "watch": "better-npm-run watch",
        "build": "better-npm-run build"
    },
    "betterScripts": {
        "start": {
            "command": "node ./dev-server.js",
            "env": {
                "NODE_ENV": "development",
                "OUTPUT_DIR": "./dev",
                "HOT_RELOAD": "true",
                "OUTPUT_PUBLIC_PATH": "."
            }
        },
        "watch": {
            "command": "webpack-cli --watch --progress --colors",
            "env": {
                "NODE_ENV": "development",
                "OUTPUT_DIR": "./dev",
                "OUTPUT_PUBLIC_PATH": "."
            }
        },
        "build": {
            "command": "webpack-cli --progress --colors",
            "env": {
                "NODE_ENV": "production",
                "OUTPUT_DIR": "./dist",
                "OUTPUT_PUBLIC_PATH": "."
            }
        }
    },
