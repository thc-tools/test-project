        "@babel/runtime": "7.0.0-beta.54",
        "prop-types": "15.6.2",
        "react": "16.4.1",
        "react-dom": "16.4.2",
        "rmwc": "1.8.3"
    },
    "devDependencies": {
        "@thc/babel-preset-react": "0.1.0-alpha.6",
        "@thc/webpack-react": "0.1.0-alpha.8",
        "better-npm-run": "0.1.0",
        "webpack": "4.16.1"
    }
