{
    "name": "test-project",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "better-npm-run start",
        "watch": "better-npm-run watch",
        "build": "better-npm-run build"
    },
    "betterScripts": {
        "start": {
            "command": "webpack-dashboard -- webpack --watch",
            "env": {
                "NODE_ENV": "development",
                "OUTPUT_DIR": "./dev",
                "OUTPUT_PUBLIC_PATH": ".",
                "HOT_RELOAD": "true"
            }
        },
        "watch": {
            "command": "webpack-dashboard -- webpack --watch --progress --color",
            "env": {
                "NODE_ENV": "development",
                "OUTPUT_DIR": "./dev",
                "OUTPUT_PUBLIC_PATH": "."
            }
        },
        "build": {
            "command": "webpack-dashboard -- webpack --progress --color",
            "env": {
                "NODE_ENV": "production",
                "OUTPUT_DIR": "./dist",
                "OUTPUT_PUBLIC_PATH": "."
            }
        }
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "prop-types": "15.7.2",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@thc/webpack-atom-js-react-css": "1.0.1-beta.9",
        "better-npm-run": "0.1.1"
    }
}
